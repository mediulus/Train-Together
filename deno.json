{
  "imports": {
    "@concepts/": "./src/concepts/",
    "@concepts": "./src/concepts/concepts.ts",
    "@test-concepts": "./src/concepts/test_concepts.ts",
    "@utils/": "./src/utils/",
    "@engine": "./src/engine/mod.ts",
    "@syncs": "./src/syncs/syncs.ts",
    "@std/assert": "jsr:@std/assert@^1.0.6",
    "@std/fs": "jsr:@std/fs@^1.0.6",
    "@std/cli/parse-args": "jsr:@std/cli@^1.0.6/parse-args",
    "@std/path/to-file-url": "jsr:@std/path@^1.0.6/to-file-url",
    "hono": "jsr:@hono/hono@^4.6.10",
    "hono/cors": "jsr:@hono/hono@^4.6.10/cors",
    "google-auth-library": "npm:google-auth-library@^10.4.0",
    "googleapis": "npm:googleapis@^162.0.0",
    "mongodb": "npm:mongodb@^6.10.0",
    "jose": "npm:jose@^5.9.3"
  },
  "nodeModulesDir": "auto",
  "tasks": {
    "start": "deno run --allow-net --allow-write --allow-read --allow-sys --allow-env src/main.ts",
    "concepts": "deno run --allow-net --allow-read --allow-sys --allow-env --env-file=.env src/concept_server.ts --port 8000 --baseUrl /api",
    "import": "deno run --allow-read --allow-write --allow-env src/utils/generate_imports.ts",
    "build": "deno run import"
  },
  "lint": {
    "rules": {
      "exclude": [
        "no-import-prefix",
        "no-unversioned-import"
      ]
    }
  }
}
